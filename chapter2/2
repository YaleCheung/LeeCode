#include <map>
#include <vector>
#include <utility>
using namespace std;

class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        multimap<int, unsigned int> hash;
        int i = 0;
        for(auto num : nums) {
            hash.emplace(std::make_pair(num, i ++));
        }
        
    }
};
