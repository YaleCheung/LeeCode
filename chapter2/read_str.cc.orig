#include <iostream>
#include <string>
#include <sstream>
using namespace std;

class Solution {
string _read_str(const string& in) {
    size_t size = in.size();
    char pre_char = in.at(0);
    int count = 0;
    stringstream ss;
    string ret = "";
    for (auto i = 0; i < size; ++ i) {
        if (in.at(i) == pre_char)
            ++ count;
        if ((i == (size - 1)) || (in.at(i) != pre_char)) {
            ss << count;
            string str_count;
            ss >> str_count;
            ret += str_count + pre_char;
            pre_char = in.at(i);
            count = 1;
        }
    }
    return ret;
}
