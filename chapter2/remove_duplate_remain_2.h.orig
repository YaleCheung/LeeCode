#include <iostream>

using namespace std;

int RemoveDuplates(int array[], const int& size) {
		if(size <= 2)
				    return size;
			int count = 2;
			    for(int i = 2; i < size; i ++) {
				    	    if(array[i] != array[count - 2])
						    	        array[count ++] = array[i];
					        }
			        return count;
}

